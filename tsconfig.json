{
  "baseUrl": "types",
  "typeRoots": ["types"],
  "compilerOptions": {    
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode":true,
    "allowUnusedLabels":true,
    "baseUrl":"src/",
    "declaration":false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es2017"],
    "listFiles":false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny":false,
    "noImplicitReturns":false,
    "noImplicitUseStrict":false,
    "noLib":false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir":"dist/",
    "pretty":true,
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap":true,
    "target": "es2017",
  },
  "include": ["./**/*.ts"],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*",
    "prisma/**/*"
  ]
}